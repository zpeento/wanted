<!--index.wxml-->
<view class="container">
  <view wx:for="{{panel}}" wx:key="*this">
    <view class="panel" id="{{item.id}}" bindtap="bindViewTap">
      <view class="panel-image">
        <image mode="aspectFill" src="{{item.imgsrc}}"></image>
      </view>
      <view class="panel-text">
        <text>{{item.text}}</text>
      </view>
    </view>
  </view>
</view>
